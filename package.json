    "electron:dev": "REACT_APP_DIST=electron REACT_APP_ENV=dev electron-forge start",
    "dev": "concurrently \"yarn webapp:dev\" \"yarn electron:dev\"",
    "package": "REACT_APP_DIST=electron REACT_APP_ENV=prod electron-forge package",
    "make": "REACT_APP_DIST=electron REACT_APP_ENV=prod electron-forge make",
    "publish": "REACT_APP_DIST=electron REACT_APP_ENV=prod electron-forge publish"
  },
