    "extension:dev": "webpack --config build-config/webpack/webpack.chrome-extension.config.js --mode=development --watch",
    "extension:build": "webpack --config build-config/webpack/webpack.chrome-extension.config.js --mode=production",
    "webapp:dev": "PORT=3001 REACT_APP_DIST=webapp REACT_APP_ENV=dev react-scripts start",
